{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [],
  "globalPassThroughEnv": ["NODE_ENV", "CI", "VERCEL", "VERCEL_ENV", "VERCEL_URL", "npm_lifecycle_event"],
  "tasks": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"],
      "persistent": false
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"],
      "persistent": true
    },
    "format": {
      "outputLogs": "new-only",
      "outputs": [".cache/.prettiercache"]
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "ui": "tui"
}
